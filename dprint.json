{
  "typescript": {
    "useTabs": true,
    "quoteStyle": "preferSingle",
    "method.spaceBeforeParentheses": true,
    "functionExpression.spaceBeforeParentheses": true,
    "operatorPosition": "sameLine"
  },
  "json": {
    "useTabs": true
  },
  "markdown": {},
  "includes": [
    "**/*.{ts,tsx,js,jsx,cjs,mjs,json,md}"
  ],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/*.lock"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.71.2.wasm",
    "https://plugins.dprint.dev/json-0.15.5.wasm",
    "https://plugins.dprint.dev/markdown-0.14.0.wasm"
  ]
}